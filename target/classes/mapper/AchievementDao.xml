	<!-- 关于IURP产学研表的查询 -->
	<select id="queryIURP" resultMap="IURPMap">
		SELECT * FROM
		achievement_industry_university_research_project;
	</select>
	<select id="queryIURPById" resultMap="IURPMap" parameterType="java.lang.String">
		SELECT * FROM achievement_industry_university_research_project
		<where>
			project_id = #{achievementId}
		</where>
		;
	</select>
	<select id="queryIURPByName" resultMap="IURPMap" parameterType="java.lang.String">
		SELECT * FROM achievement_industry_university_research_project
		<where>
			project_name
			LIKE CONCAT(CONCAT("%", #{search}), "%")
		</where>
		;
	</select>
	<select id="queryIURPByAuthor" resultMap="IURPMap"
		parameterType="java.lang.String">
		SELECT * FROM achievement_industry_university_research_project
		<where>
			compere
			LIKE CONCAT(CONCAT("%", #{search}), "%")
		</where>
		;
	</select>
	<insert id="insertIURP" parameterType="cn.edu.tit.bean.IURP">
		INSERT INTO
		achievement_industry_university_research_project
		(project_id,
		project_name,
		project_detail,
		compere,
		member,
		collect_the_amount,
		cooperator,
		start_time,
		end_time,
		status,
		first_picture,
		is_share,
		project_category,
		introduction,
		browse_volume,
		price)
		VALUES
		(#{iurp.projectId},
		#{iurp.projectName},
		#{iurp.projectDetail},
		#{iurp.compere},
		#{iurp.member},
		#{iurp.collectTheAmount},
		#{iurp.cooperator},
		#{iurp.startTime},
		#{iurp.endTime},
		#{iurp.status},
		#{iurp.firstPicture},
		#{iurp.isshare},
		#{iurp.projectCategory},
		#{iurp.introduction},
		#{iurp.browseVolume},
		#{iurp.price}
		);
	</insert>


